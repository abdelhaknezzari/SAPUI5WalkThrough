<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" controllerName="na.myProject2.controller.InvoiceList">
	<List class="sapUiResponsiveMargin" width="auto" items="{invoice>/Invoices}">
		<items>
			<ObjectListItem title="{invoice>Quantity}x{invoice>ProductName}"
				number="{ parts:[{path:'invoice>ExtendedPrice'},{path:'view>/currency'}], type:'sap.ui.model.model.type.Currency', formatOptions:{showMesure:false} }"
				numberUnit="{view>/currency}"/>
		</items>
	</List>
</mvc:View>